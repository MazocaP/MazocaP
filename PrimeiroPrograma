package primeiroprograma;
public class PrimeiroPrograma {
    public static void main(String[] args) {
        
        Caneta c1 = new Caneta();
        c1.modelo = "Bic";
        c1.setCor("Vermelha");
        //c1.ponta = 0.5f;
        c1.carga = 34;
        c1.tampada = false;
        c1.status();
        c1.rabiscar();
    }
}
